<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cz.pvsps.corsitask.corsitest.StartTestController">
    <center>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: black;"
              BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0"
                        text="START" fx:id="startButton" onMouseClicked="#startButtonOnMouseClicked"
                        onMouseEntered="#startButtonOnMouseEntered" onMouseExited="#startButtonOnMouseExited"
                        style="-fx-background-color: yellow; -fx-border-color: yellow; -fx-border-width: 5;">
                    <font>
                        <Font name="Arial Black" size="36.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
    </center>
</BorderPane>
